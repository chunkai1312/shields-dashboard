{
  "name": "universal-react-redux-starter-kit",
  "version": "0.0.0",
  "description": "A starter kit for building universal React/Redux apps",
  "scripts": {
    "start": "npm run build && npm run prod",
    "prod": "npm run start:prod",
    "dev": "concurrently -k \"npm run build:watch\" \"npm run start:dev\"",
    "build": "npm run build:clean && npm run build:server && npm run build:client",
    "start:prod": "cross-env NODE_ENV=production node bin/server.js",
    "start:dev": "nodemon bin/server.js",
    "build:clean": "rimraf dist static/dist",
    "build:server": "babel src -d dist --copy-files",
    "build:client": "webpack --config build/webpack.prod.config.js --progress",
    "build:watch": "node build/dev-server.js",
    "test": "cross-env NODE_ENV=test jest --config .jestrc --forceExit",
    "test:coverage": "npm test -- --coverage",
    "test:watch": "npm test -- --watch",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chunkai1312/universal-react-redux-starter-kit.git"
  },
  "keywords": [
    "universal",
    "isomorphic",
    "react",
    "reactjs",
    "redux",
    "boilerplate",
    "starter",
    "webpack"
  ],
  "author": "Chun-Kai Wang <chunkai1312@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chunkai1312/universal-react-redux-starter-kit/issues"
  },
  "homepage": "https://github.com/chunkai1312/universal-react-redux-starter-kit",
  "dependencies": {
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "express": "^4.14.1",
    "http-proxy-middleware": "^0.17.3",
    "postcss": "^5.2.15",
    "postcss-cssnext": "^2.9.0",
    "react": "~15.4.0",
    "react-dom": "~15.4.0",
    "react-helmet": "^4.0.0",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-toolbox": "^2.0.0-beta.8",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.2",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "^2.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.3",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "concurrently": "^3.3.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "enzyme": "^2.8.0",
    "enzyme-to-json": "^1.5.0",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^19.0.2",
    "nodemon": "^1.11.0",
    "postcss-each": "^0.9.3",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.1",
    "postcss-mixins": "^5.4.1",
    "react-addons-css-transition-group": "~15.4.0",
    "react-addons-test-utils": "~15.4.0",
    "react-hot-loader": "next",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1",
    "webpack-isomorphic-tools": "^2.6.6"
  }
}
